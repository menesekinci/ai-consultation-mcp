{
  "name": "ai-consultation-mcp",
  "version": "2.1.4",
  "type": "module",
  "description": "MCP server for consulting other AI models (DeepSeek, OpenAI) for alternative perspectives during coding sessions",
  "main": "./dist/index.js",
  "bin": {
    "ai-consultation-mcp": "./dist/index.js",
    "ai-consultation-daemon": "./dist/daemon/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:daemon": "tsx watch src/daemon/index.ts",
    "build": "tsc && cp -r src/ui dist/",
    "build:ui": "cp -r src/ui dist/",
    "start": "node dist/index.js",
    "start:config": "node dist/index.js --config",
    "start:daemon": "node dist/daemon/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "llm",
    "openai",
    "deepseek",
    "claude-code",
    "agent",
    "consultation"
  ],
  "author": "Menes Ekinci",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/menesekinci/ai-consultation-mcp"
  },
  "homepage": "https://github.com/menesekinci/ai-consultation-mcp#readme",
  "bugs": {
    "url": "https://github.com/menesekinci/ai-consultation-mcp/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.38.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "better-sqlite3": "^11.0.0",
    "express": "^5.2.1",
    "mammoth": "^1.7.2",
    "multer": "^1.4.5-lts.1",
    "open": "^11.0.0",
    "openai": "^6.16.0",
    "pdf-parse": "^1.1.1",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "uuid": "^11.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^5.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.0.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
